{
  "name": "rets.js",
  "version": "0.0.4",
  "description": "Simplified RETS Node Client",
  "keywords": [
    "rets",
    "mls",
    "real estate",
    "javascript",
    "node",
    "reso",
    "Real Estate Transaction Standard",
    "real estate transaction standard",
    "odata",
    "retsly",
    "zillow",
    "trulia",
    "rental",
    "housing"
  ],
  "main": "index.js",
  "dependencies": {
    "MD5": "^1.2.1",
    "event-stream": "^3.3.0",
    "inflected": "^1.1.6",
    "lodash": "^3.5.0",
    "request": "^2.53.0",
    "sax": "^0.6.1",
    "xml2js": "^0.4.6"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "debug": "^2.1.3",
    "glob": "^5.0.3",
    "istanbul": "^0.3.8",
    "jsdoc": "^3.3.0-beta3",
    "jsdox": "^0.4.9",
    "jshint": "^2.6.3",
    "mocha": "^2.2.1",
    "nock": "^1.2.0",
    "nodemon": "^1.3.7",
    "should": "^5.2.0"
  },
  "scripts": {
    "clean": "rm -rf test/tmp; rm -rf docs",
    "test": "rm -rf test/tmp; ./node_modules/.bin/mocha test/index",
    "test-servers": "rm -rf test/tmp; ./node_modules/.bin/mocha test/servers",
    "test-watch": "DEBUG=rets.js:* rm -rf test/tmp; ./node_modules/.bin/mocha --watch --recursive --reporter spec test/index",
    "jsdoc": "jsdoc -X lib",
    "docs": "npm run clean && ./node_modules/.bin/jsdox --output docs lib",
    "lint": "find . -name '*.js' ! -path './node_modules/*' -print0 | xargs -0 ./node_modules/.bin/jshint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/retsr/rets.js.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/retsr/rets.js/issues"
  },
  "homepage": "http://retsr.io/rets.js/",
  "engines": {
    "node": ">=0.12"
  }
}
